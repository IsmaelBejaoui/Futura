@model Futura.Models.Entwickler

@{
    ViewBag.Title = "Create";
}

<h2>Erstellen</h2>


@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
        <h4>Entwickler</h4>
        <hr />
        @Html.ValidationSummary(true)

        

        <div class="form-group">
            <h5>
                @Html.LabelFor(model => model.Enwicklername)
            </h5>            
                @Html.TextBoxFor(model => model.Enwicklername, new { @class = "form-control" })
                @Html.ValidationMessageFor(model => model.Enwicklername)            
        </div>

        <div class="form-group">         
            <input type="submit" value="Erstellen" class="btn btn-default" />         
        </div>
    </div>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
